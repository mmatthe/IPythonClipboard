{
    "manifest_version": 2,

    "name": "AutoIPython",
    "description": "This extension copies marked text to a cell in a new IPython notebook.",
    "version": "1.0",

    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "main.html"
    },
    "permissions": [
	"activeTab",
	"http://localhost/",
        "clipboardWrite",
        "clipboardRead"
	]
}
